{
  "cli": {
    "requireCommit": false
  },
  "build": {
    "production": {
      "ios": {
        "releaseChannel": "production-2.1.0"
      }
    },
    "preview": {
      "ios": {
        "releaseChannel": "staging-2.1.1-45"
      }
    },
    "simulator": {
      "ios": {
        "simulator": true
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "${EXPO_APPLE_ID}",
        "appleTeamId": "${EXPO_APPLE_TEAM_ID}",
        "ascAppId": "${EXPO_ASC_APP_ID}"
      }
    },
    "preview": {
      "ios": {
        "appleId": "${EXPO_APPLE_ID}",
        "appleTeamId": "${EXPO_APPLE_TEAM_ID}",
        "ascAppId": "${EXPO_ASC_APP_ID}"
      }
    }
  }
}
