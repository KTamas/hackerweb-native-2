{
  "name": "hackerweb",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "current-version": "node -e 'console.log(require(\"./app.json\").expo.version)'",
    "eas-build": "eas build -p ios --profile=preview",
    "eas-build-sim": "eas build -p ios --profile=simulator",
    "build": "expo build:ios --release-channel staging-`npm run current-version --silent`",
    "eas-build-prod": "eas build -p ios",
    "build-prod": "expo build:ios --release-channel production-`npm run current-version --silent`",
    "latest-staging": "expo publish:history -p ios --count 1 -r -c staging-`npm run current-version --silent`",
    "latest-publish-id": "node -e \"console.log($(npm run latest-staging --silent).queryResult[0].publicationId)\"",
    "publish": "expo publish --release-channel staging-`npm run current-version --silent`",
    "promote-prod": "expo publish:set --publish-id `npm run latest-publish-id --silent` --release-channel production-`npm run current-version --silent`",
    "viz": "npx react-native-bundle-visualizer --expo managed"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/hooks": "^2.6.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-segmented-control/segmented-control": "2.3.0",
    "@react-navigation/native": "^5.9.8",
    "@react-navigation/stack": "^5.14.9",
    "array-move": "~3.0.1",
    "buffer": "~6.0.3",
    "date-fns": "~2.21.0",
    "entities": "~2.2.0",
    "expo": "^42.0.0",
    "expo-blur": "~9.0.3",
    "expo-constants": "~11.0.1",
    "expo-device": "~3.3.0",
    "expo-haptics": "~10.1.0",
    "expo-linear-gradient": "~9.2.0",
    "expo-linking": "~2.3.1",
    "expo-mail-composer": "~10.2.2",
    "expo-status-bar": "~1.0.4",
    "expo-store-review": "~4.1.0",
    "expo-updates": "~0.8.2",
    "expo-web-browser": "~9.2.0",
    "htmlparser2": "~6.1.0",
    "ky": "~0.27.0",
    "p-memoize": "~4.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-modalize": "^2.0.8",
    "react-native-reanimated": "~2.2.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.4.0",
    "react-native-svg": "12.1.1",
    "react-native-url-polyfill": "^1.2.0",
    "react-native-web": "~0.13.12",
    "react-native-webview": "11.6.2",
    "stream": "0.0.2",
    "url-regex-safe": "~2.0.2",
    "zustand": "~3.4.1"
  },
  "devDependencies": {
    "@babel/core": "~7.13.0",
    "react-native-svg-transformer": "~0.14.3"
  }
}
