{
  "name": "hackerweb",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "current-version": "jq '.expo.version' app.json -r",
    "last-eas-build": "eas build:list --limit=1 --buildProfile=preview --json | jq -r .[0].appBuildVersion",
    "current-eas-build": "jq -r '.expo.ios.buildNumber' app.json",
    "eas-build": "eas build -p ios --profile=preview",
    "eas-build-ci": "eas build -p ios --profile=preview --non-interactive",
    "preeas-build-submit-ci": "envsubst < eas.json > eas.tmp && mv eas.tmp eas.json",
    "eas-build-submit-ci": "eas build -p ios --profile=preview --non-interactive --auto-submit",
    "eas-build-sim": "eas build -p ios --profile=simulator",
    "build": "expo build:ios --release-channel staging-`npm run current-version --silent`",
    "eas-build-prod": "eas build -p ios",
    "eas-build-submit-prod": "eas build -p ios --auto-submit",
    "build-prod": "expo build:ios --release-channel production-`npm run current-version --silent`",
    "latest-staging": "expo publish:history -p ios --count 1 -r -c staging-`npm run current-version --silent`",
    "latest-publish-id": "npm run latest-staging --silent | jq '.queryResult[0].publicationId' -r",
    "publish": "expo publish --release-channel staging-`npm run current-version --silent`",
    "promote-prod": "expo publish:set --publish-id `npm run latest-publish-id --silent` --release-channel production-`npm run current-version --silent`",
    "viz": "npx react-native-bundle-visualizer --expo managed",
    "preeas-submit": "set -a && source .env && set +a && envsubst < eas.json > eas.tmp && mv eas.tmp eas.json",
    "eas-submit": "eas submit -p ios --profile=preview --latest",
    "posteas-submit": "git checkout eas.json",
    "eas-submit-ci": "eas submit -p ios --latest --non-interactive",
    "build-publish": "if [ $(npm run last-eas-build --silent) = $(npm run current-eas-build --silent) ]; then npm run publish; else npm run eas-build-submit-ci; fi"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/hooks": "~2.6.0",
    "@react-native-masked-view/masked-view": "0.2.5",
    "@react-native-segmented-control/segmented-control": "2.4.0",
    "@react-navigation/native": "~6.0.6",
    "@react-navigation/native-stack": "~6.2.5",
    "@react-navigation/stack": "~6.0.11",
    "array-move": "~4.0.0",
    "buffer": "~6.0.3",
    "date-fns": "~2.23.0",
    "entities": "~3.0.1",
    "expo": "^43.0.0",
    "expo-blur": "~10.0.3",
    "expo-constants": "~12.1.3",
    "expo-device": "~4.0.3",
    "expo-haptics": "~11.0.3",
    "expo-linear-gradient": "~10.0.3",
    "expo-linking": "~2.4.2",
    "expo-mail-composer": "~11.0.3",
    "expo-status-bar": "~1.1.0",
    "expo-store-review": "~5.0.3",
    "expo-updates": "~0.10.15",
    "expo-web-browser": "~10.0.3",
    "htmlparser2": "~7.1.2",
    "ky": "~0.28.5",
    "p-memoize": "~4.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-modalize": "~2.0.8",
    "react-native-reanimated": "~2.2.0",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.8.0",
    "react-native-svg": "12.1.1",
    "react-native-url-polyfill": "~1.2.0",
    "react-native-web": "0.17.1",
    "react-native-webview": "11.13.0",
    "stream": "0.0.2",
    "strip-indent": "~4.0.0",
    "url-regex-safe": "~2.0.2",
    "zustand": "~3.5.10"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "react-native-svg-transformer": "~0.14.3"
  }
}
